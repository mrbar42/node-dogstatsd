{
  "name": "abdogstatsd",
  "description": "node client for extended StatsD server of Datadog",
  "version": "0.0.8",
  "author": "Young Han Lee, Aaron Brady",
  "repository": {
    "type": "git",
    "url": "git://github.com/abrady0/node-dogstatsd.git"
  },
  "bugs": {
    "url": "https://github.com/abrady0/node-dogstatsd/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive --require should",
    "lint": "jshint && flow/flow"
  },
  "directories": {
    "lib": "./lib/"
  },
  "main": "./lib/statsd",
  "engines": {
    "node": ">=0.1.97"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/joybro/node-dogstatsd/raw/master/LICENSE"
    }
  ],
  "devDependencies": {
    "mocha": "^2.2.5",
    "should": "^6.0.3"
  },
  "jshintConfig": {
    "browser": false,
    "curly": false,
    "debug": true,
    "indent": 2,
    "esnext": false,
    "expr": true,
    "globalstrict": false,
    "latedef": "nofunc",
    "node": true,
    "nonbsp": true,
    "shadow": false,
    "undef": true,
    "unused": "vars",
    "predef": [
      "process",
      "Buffer",
      "webkit",
      "console",
      "require",
      "exports",
      "module",
      "CryptoJS",
      "IDBKeyRange",
      "appRequire"
    ]
  }
}
